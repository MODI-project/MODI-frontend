{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src",
    "vite-env.d.ts",
    "src/types/**/*",
    "vite.config.ts",
    "vite-plugin-svgr.d.ts",
    "react-mobile-picker-scroll.d.ts"
  ]
}
